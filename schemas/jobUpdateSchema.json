{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "Root Schema",
    
    "properties": {
        "title": {
            "type": "string",
            "minLength": 1,
            "maxLength": 30
        },
        "salary": {
            "type": "integer",
            "minimum": 0
        },
        "equity": {
            "type": "string",
            "pattern": "0|(0?\\.[0-9]+)"
        }
    },
    "examples": [
        {
            "title": "jtitle",
            "salary": 100,
            "equity": 0.1
        }
    ]
}